citHeader("To cite package 'msgtools' in publications use:")
     
 year <- sub(".*(2[[:digit:]]{3})-.*", "\\1", meta$Date, perl = TRUE)
 vers <- paste("R package version", meta$Version)
 
 citEntry(entry="Manual",
		  title = "msgtools: Tools for developing diagnostic messages",
		  author = personList(as.person("Thomas J. Leeper")),
		  year = year,
		  note = vers,
		  textVersion =
		  paste("Thomas J. Leeper (",
				year,
				"). msgtools: Tools for developing diagnostic messages. ",
				vers, ".", sep=""))